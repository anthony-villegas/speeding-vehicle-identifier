target Python;

reactor ImageUploader {
    preamble {=
        import boto3
        import os
        s3_client = boto3.client("s3")
        bucket_name = "license-plate-images-bucket"
    =}

    input image;

    reaction(image) {=
        self.s3_client.put_object(Bucket=self.bucket_name, Key=str(image.value.timestamp) + ".jpg", Body=image.value.body_string)
        print("Uploaded\n")
    =}
}